//Sum of 3 squaers equals 2019
#include <stdio.h>

int main()
{
    printf("Hello World\n");
    int i,j,k;
    
    for (i=1; i<45; i++) {
        for (j=i+1; j<45; j++) {
            for (k=j+1; k<45; k++) {
                if(i!=j && i!=k && j!=k) {
                    
                    if((i*i + j*j + k*k) == 2019) printf("%d %d %d\n", i,j,k);
                    //sleep(1);
                }
            }
        }
    }
    

    return 0;
}
